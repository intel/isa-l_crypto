#ifndef _ISAL_CRYPTO_H_
#define _ISAL_CRYPTO_H_

#define ISAL_CRYPTO_MAJOR_VERSION @PROJECT_VERSION_MAJOR@
#define ISAL_CRYPTO_MINOR_VERSION @PROJECT_VERSION_MINOR@
